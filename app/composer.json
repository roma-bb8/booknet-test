{
    "name": "roma-bb8/book-net-test",
    "description": "BookNet Test",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.4",
        "symfony/console": "^7.2",
        "symfony/orm-pack": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
